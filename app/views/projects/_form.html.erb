<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :image_url %>
    <%= f.input :link_url %>
    <%= f.input :about %>
    <%= f.input :name %>
    <%= f.association :user %>
    <%= f.association :cohort %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
