<h3>Golden Graduates</h3>
<% @users.each do |user| %>
<div class="span6">
  <p><% unless user.image_url.blank? %><%= image_tag( user.image_url ) %><% end %></p>
  <p class="lead"><%= link_to user.name, user %></p>
      <p><%= user.about %></p>
      <p><%= link_to user.website_url, url_with_protocol(user.website_url) %></p>
      <p><%= link_to user.linkedin_url, url_with_protocol(user.linkedin_url) %></p>
      <p><%= user.cohort.name if user.cohort %></p>
  </div>
<% end %>

<h3>Awesome Apps</h3>
<% @projects.each do |project| %>
    <td><%= project.image_url %></td>
    <td><%= project.link_url %></td>
    <td><%= project.about %></td>
    <td><%= project.name %></td>
    <td><%= project.cohort %></td>
  </tr>
<% end %>